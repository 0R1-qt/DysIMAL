{
	"QCustomSlideMenu": [{
		"name": "sideMenu",
		"defaultSize": [{
			"width": 120,
			"height": "auto"
		}],
		"collapsedSize": [{
			"width": 0,
			"height": 0
		}],
		"expandedSize": [{
			"width": 120,
			"height": "auto"
		}],
		"toggleButton": [{
			"buttonName": "sideMenuBtn",
			"icons": [{
				"whenMenuIsCollapsed": ":/icons/1CON/align-justify.svg",
				"whenMenuIsExpanded": ":/icons/1CON/chevron-left.svg"
			}]
		}],
		"menuTransitionAnimation": [{
			"animationDuration": 300
		}]
	}]
}